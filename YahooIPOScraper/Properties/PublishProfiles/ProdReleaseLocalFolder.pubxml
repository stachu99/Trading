<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>FileSystem</WebPublishMethod>
    <PublishProvider>FileSystem</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish />
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <ProjectGuid>dfe3c96b-7479-47f1-ade2-d975a9f5fa30</ProjectGuid>
    <publishUrl>D:\Projects\!Publish\Trading\Production\Release\YahooIPOScraper</publishUrl>
    <DeleteExistingFiles>True</DeleteExistingFiles>
    <TargetFramework>netcoreapp2.0</TargetFramework>

    <SolutionName>Trading</SolutionName>
    <Environment>Production</Environment>
    <SolutionDir>D:\Projects\Trading\</SolutionDir>
  </PropertyGroup>

  <Target Name="AfterPublishEvents" AfterTargets="AfterPublish">
    <Exec Command="copy /Y $(SolutionDir)!SensitiveData\$(ProjectName)\appSettings.$(Environment).json D:\Projects\!Publish\$(SolutionName)\$(Environment)\$(ConfigurationName)\$(ProjectName)" />
    <Exec Command="del /Q /F D:\Projects\!Publish\$(SolutionName)\$(Environment)\$(ConfigurationName)\*" />
    <Exec Command="copy /Y $(SolutionDir)!SensitiveData\DockerConfig\$(Environment)\* D:\Projects\!Publish\$(SolutionName)\$(Environment)\$(ConfigurationName)\" />
  </Target>

</Project>